<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="jp.arise.gbj.dao.GBJGM002Dao">
    <resultMap id="resultGBJGM002Dto" type="jp.arise.gbj.dto.GBJGM002Dto">
        <result column="username" property="user"/>
        <result column="userid" property="userId"/>
    </resultMap>

    <select id="getUser" resultMap="resultGBJGM002Dto">
        SELECT username
        FROM test
        WHERE userid = #{userId}
    </select>

	<insert id= "insert" parameterType= "jp.arise.gbj.dto.GBJGM002Dto">
	INSERT INTO genba_dt (genba_id,genba_na,genba_address,genba_moyori_eki_1,genba_moyori_eki_2,genba_moyori_eki_3,sagyonaiyo,phase_cd,pj_id,yuko_fg,sakujo_fg)
	VALUES(#{genba_Id},#{genba_na},#{address},#{moyori_1_station},#{moyori_2_station},#{moyori_3_station},#{work},#{phase},#{pi_id},#{yuko_fg},#{sakujo_fg})
	</insert>

	<delete id= "delete" parameterType= "jp.arise.gbj.dto.GBJGM002Dto">
	DELETE FROM genba_dt
	WHERE genba_id = #{genba_Id}
	</delete>


</mapper>